   ],
   "source": [
    "from datetime import date, timedelta\n",
    "\n",
    "birth_date = date(1996, 3, 21)\n",
    "birthday_10000 = birth_date + timedelta(days=10000)\n",
    "reference = date(2100, 9, 1)\n",
    "(reference-birthday_10000).days"
   ]
  }
