   "source": [
    "Generate an array:\n",
    "\n",
    "- of shape (10, 20, 5)\n",
    "- filled with uniformely randomly distributed values between 42 and 101 (float values)\n",
    "- given an initial state 10"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "619b8b99",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy.random as npr\n",
    "\n",
    "rng = npr.default_rng(10)                 \n",
    "arr = 42 + rng.random((10, 20, 5)) * (101-42)\n",
    "arr"
   ]
  }
